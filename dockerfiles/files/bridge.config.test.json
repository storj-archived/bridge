          "exclusive": false,
          "durable": true,
          "arguments": {
            "messageTtl": 120000
          }
        }
      },
      "callback": {
        "name": "",
        "options": {
          "exclusive": true,
          "durable": false
        }
      }
    },
    "exchanges": {
      "events": {
        "name": "storj.events",
        "type": "topic",
        "options": {
          "durable": true
        }
      }
    }
  }
}
